#include <stdio.h>
#include <stdlib.h>
int media=0;
int funcao(int n){
int i, vetor[n], soma=0;
    for(i=0;i<n;i++){
        printf("Digite o valor %d: " , i+1);
        scanf("%d" , &vetor[i]);
    }
    for(i=0;i<n;i++){
        soma+=vetor[i];
    }
    media=soma/n;
}
int main()
{
    int i, n;
    printf("Digite o tamanho do vetor: ");
    scanf("%d" , &n);
    funcao(n);
    printf("Media: %d" , media);

return 0;
}
