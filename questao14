#include <stdio.h>
#include <math.h>
int quadrado(int x, int z){
    int quadrado=pow(x,z);
    return printf("%d" , quadrado);
}

int main()
{
    int x, z;
    printf("Digite o valor x: ");
    scanf("%d" , &x);
    printf("\nDigite o valor z: ");
    scanf("%d" , &z);
    quadrado(x,z);
    return 0;
}
